   "source": [
    "# Creating and saving a binary fst"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "from fst import StdVectorFst\n",
    "\n",
    "fst = StdVectorFst()\n",
    "\n",
    "fst.start = fst.add_state()\n",
    "\n",
    "fst.add_arc(0, 1, 1, 1, 0.5)\n",
    "fst.add_arc(0, 1, 2, 2, 1.5)\n",
    "\n",
    "fst.add_state()\n",
    "fst.add_arc(1, 2, 3, 3, 2.5)\n",
    "\n",
    "fst.add_state()\n",
    "fst[2].final = 3.5\n",
    "\n",
    "fst.write('binary.fst')"
   ]
  }
